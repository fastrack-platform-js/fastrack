{
  "name": "fastrack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastrack-platform-js/fastrack.git"
  },
  "homepage": "https://github.com/fastrack-platform-js/fastrack#readme",
  "bugs": {
    "url": "https://github.com/fastrack-platform-js/fastrack/issues"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "build:examples": "pnpm -r run build --filter './examples/*'",
    "test": "pnpm -r run test --filter './packages/*'",
    "lint": "pnpm -r run lint --filter './packages/*'",
    "clean": "pnpm -r run clean --filter './packages/*'",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && pnpm test && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  }
}
